{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "prod/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/fees",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/prod/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": null,
  "outputDirectory": null
} 